{
  "name": "@arnaud-barre/rds",
  "version": "0.5.2",
  "license": "MIT",
  "scripts": {
    "dev": "scripts/bundle.ts --dev",
    "build": "scripts/bundle.ts",
    "tsc": "tsc",
    "lint": "bun lint-ci --fix --cache",
    "lint-ci": "eslint src scripts --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "prettier": "bun prettier-ci --write",
    "prettier-ci": "prettier --cache --check '*.{js,json,md}' '{.github,src,scripts}/**/*.{js,ts,yml}'",
    "ci": "tsc && bun lint-ci && bun prettier-ci && bun run build"
  },
  "prettier": {
    "trailingComma": "all"
  },
  "peerDependencies": {
    "@arnaud-barre/downwind": "^0.4.2",
    "eslint": "^8",
    "typescript": "^4"
  },
  "dependencies": {
    "@arnaud-barre/config-loader": "^0.5.2",
    "@swc/core": "^1.3.22",
    "es-module-lexer": "^1.1.0",
    "chokidar": "^3.5.3",
    "esbuild": "^0.16.4",
    "ws": "^8.11.0"
  },
  "devDependencies": {
    "@arnaud-barre/downwind": "^0.4.3",
    "@arnaud-barre/eslint-config": "^1.0.18",
    "@nabla/tnode": "^0.8.0",
    "@types/eslint": "^8.4.10",
    "@types/node": "^18.11.12",
    "@types/react": "^18.0.26",
    "@types/ws": "^8.5.3",
    "eslint": "^8.29.0",
    "prettier": "^2.8.1",
    "typescript": "^4.9.4"
  }
}
